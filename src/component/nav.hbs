
{{#if isVertical}}
<div class="nav">
    <div class="nav_vertical">
    {{#each list}}
     <a href="{{url}}" class="nav_link">{{text}}</a>
    {{/each}}
    </div>
</div>
{{else}}
<div class="nav">
    <div class="nav_inline">
    {{#each list}}
     <a href="{{url}}" class="nav_link">{{text}}</a>
    {{/each}}
    </div>
</div>
{{/if}}


